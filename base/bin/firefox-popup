#!/bin/sh
# variables
if [ -p ~/pipe:sh ]; then
	exec 2> ~/pipe:sh
	set -x
fi
MEDIA_PLAYER=${MEDIA_PLAYER:-mpv}
WIKIPEDIA_APP=${WIKIPEDIA_APP:-wike}
if [ -z "$*" ]; then
	exec firefox "$@"
fi
if
	hash "$MEDIA_PLAYER" > /dev/null &&
		echo "$1" | grep -Eq \
			'^https?://(www\.)?youtu(\.be/|be\.com/(watch\?(.+&)?v=|playlist\?list=))[a-zA-Z0-9_-]+($|&.*$|\?.*$)'
then
	case "$MEDIA_PLAYER" in
		mpv)
			mkdir -p "${XDG_RUNTIME_DIR:-/run/user/$(id -u)}/mpv"
			exec mpv \
				--script-opts=ontop-enable=no \
				--player-operation-mode=pseudo-gui \
				--cache-dir="${XDG_RUNTIME_DIR:-/run/user/$(id -u)}/mpv" \
				--cache=yes --cache-on-disk \
				--cache-unlink-files=whendone \
				--cache-pause-initial \
				-- "$@"
			;;
		*) exec "$MEDIA_PLAYER" "$@" ;;
	esac
elif
	hash "$WIKIPEDIA_APP" > /dev/null &&
		echo "$1" | grep -Eq \
			'^https?://(www|[a-z]{2})\.(m\.)?wikipedia\.org/wiki/.+'
then
	case "$WIKIPEDIA_APP" in
		wike) exec wike -u "$1" ;;
		*) exec "$WIKIPEDIA_APP" "$@" ;;
	esac
elif hash chromium > /dev/null &&
	echo "$1" | grep -Eq \
		'^(https?://[a-z]+\.google\.com/.*|https?://app\.convay\.com/m/j/[0-9]+/[a-z0-9]+(\?pwd=[a-z0-9]+)?)$'; then
	class="$(echo "$1" | sed -nE 's#^.+://(\w+)\.(google)\.com/.*$#\2-\1#p')"
	exec chromium --app="$@" --class="${class:-google-chrome}"
elif hash zoom > /dev/null && echo "$1" | grep -q 'zoom\.us/j/'; then
	set "$(echo "$1" | sed -nE 's|.+://(.+\.zoom\.us)/j/(.+)\?(.+)|zoommtg://\1/join?action=join\&confno=\2\&\3|p')"
	exec zoom "$@"
fi
end=$(
	awk '!/^### END SCRIPT ###$/{line+=1};/^### END SCRIPT ###$/{print line+2;exit}' "$0"
)
ffver=$(firefox -version | awk '{print $NF}')
search=112
places=107
profile="${XDG_DATA_HOME:-$HOME/.local/share}/firefox-popup"
config="${XDG_CONFIG_HOME:-$HOME/.config}/firefox-popup"
test -d "$profile" || mkdir -p "$profile/chrome" || exit 1
printf '%s\n' \
	"#nav-bar, #toolbar-menubar," \
	"#mainPopupSet > menupopup:not(#contentAreaContextMenu)," \
	"#identity-box, #toolbarbutton" \
	"{ visibility: collapse !important;}" > "$profile/chrome/userChrome.css"
printf '%s\n' \
	':root {' \
	'scrollbar-color: rgb(94, 94, 94, 0.55) rgb(255, 255, 255, .0) !important;' \
	'scrollbar-width: thin !important;' \
	'}' > "$profile/chrome/userContent.css"
{
	test -f "$config/user.js" && cat "$config/user.js"
	printf '%s\n' \
		'user_pref("browser.cache.disk.enable", false);' \
		'user_pref("browser.cache.disk.capacity", 0);' \
		'user_pref("browser.uiCustomization.state", "{\"placements\":{\"widget-overflow-fixed-list\":[],\"nav-bar\":[],\"toolbar-menubar\":[],\"TabsToolbar\":[\"tabbrowser-tabs\",\"customizableui-special-spring2\",\"stop-reload-button\",\"alltabs-button\"],\"PersonalToolbar\":[]},\"dirtyAreaCache\":[\"nav-bar\",\"PersonalToolbar\",\"toolbar-menubar\",\"TabsToolbar\"]}");' \
		'user_pref("browser.cache.disk.filesystem_reported", 1);' \
		'user_pref("browser.cache.disk.smart_size.enabled", false);' \
		'user_pref("browser.cache.disk.smart_size.first_run", false);' \
		'user_pref("browser.cache.disk.smart_size.use_old_max", false);' \
		'user_pref("browser.ctrlTab.previews", true);' \
		'user_pref("browser.tabs.warnOnClose", false);' \
		'user_pref("toolkit.legacyUserProfileCustomizations.stylesheets", true);'
	test "$ffver" != "$(printf '%s\n84.0\n' "$ffver" | sort --version-sort | head -n 1)" &&
		test -c /dev/nvidiactl &&
		printf 'user_pref("gfx.webrender.software", true);\n'
} > "$profile/user.js"
tail -n "+$end" "$0" | head -n $search | base64 -d > "$profile/search.json.mozlz4"
tail -n "+$end" "$0" | tail -n $places | base64 -d | zstd -d > "$profile/places.sqlite"

exec firefox --class firefox-popup -width 800 -height 600 --profile "$profile" "$*"

### END SCRIPT ###
