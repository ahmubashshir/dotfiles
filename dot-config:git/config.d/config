[init]
	defaultBranch = master
[advice]
	addIgnoredFile = false
[commit]
	gpgSign = true
	template = ~/.config/git/commit-msg
[color]
	ui = auto
[core]
	hooksPath = ~/.config/git/hooks
	editor = nano -r 72
[fetch]
	prune = true
[push]
	default = current
[pull]
	rebase = true
[log]
	decorate = short
	date = iso
[rebase]
	autoStash = true
[checkout]
	defaultRemote = origin
[branch]
	autoSetupMerge = true
